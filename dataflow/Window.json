{
	"name": "Window",
	"properties": {
		"folder": {
			"name": "Testing"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_testing_EmpData",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "window1"
				},
				{
					"name": "cast1"
				},
				{
					"name": "rank1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EMP_ID as string,",
				"          EMP_NAME as string,",
				"          GENDER as string,",
				"          DOJ as string,",
				"          SALARY as string,",
				"          DEPT_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"cast1 window(over(DEPT_ID),",
				"     asc(DEPT_ID, true),",
				"     CountOfEmployees = count(EMP_ID)) ~> window1",
				"source1 cast(output(",
				"          SALARY as integer",
				"     ),",
				"     errors: true) ~> cast1",
				"window1 rank(desc(SALARY, true),",
				"     output(OverallRank as long)) ~> rank1"
			]
		}
	}
}